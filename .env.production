# Production Environment Variables for Vercel
# Add these to your Vercel dashboard under Project Settings > Environment Variables

# Application Configuration
VITE_APP_NAME=WorkFlow Management System
VITE_APP_VERSION=1.0.0

# API Configuration - Update these for your production backend
VITE_API_BASE_URL=https://your-backend-api.vercel.app/api
VITE_API_TIMEOUT=10000

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=jpg,jpeg,png,pdf,doc,docx,txt

# Security
VITE_ENABLE_2FA=true
VITE_SESSION_TIMEOUT=1800000

# Production Mode
VITE_DEMO_MODE=false
VITE_DEBUG=false
VITE_LOG_LEVEL=warn
